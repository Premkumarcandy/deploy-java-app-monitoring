[Unit]
Description=Prometheus server
After=network.target

[Service]
WorkingDirectory=/usr/share/prometheus/prometheus-0.16.0.linux-amd64
ExecStart=/usr/share/prometheus/prometheus-0.16.0.linux-amd64/prometheus \
    -config.file=/usr/share/prometheus/prometheus-0.16.0.linux-amd64/prometheus.yml

[Install]
WantedBy=multi-user.target
